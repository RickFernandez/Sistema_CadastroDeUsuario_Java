package br.com.master3.beans;

import java.util.HashMap;

import javax.swing.JOptionPane;

public class MapCadastrarUsuario {

	public static void main(String[] args) {
		HashMap<String, String> newUsuario = new HashMap<String, String>();
//		HashMap<String, String> newUsuario = new HashMap<String, String>();
//		HashMap<String, String> newUsuario = new HashMap<String, String>();
		
		newUsuario.put(JOptionPane.showInputDialog("Por favore, informe o sue nome"), JOptionPane.showInputDialog("Por favor, informe como gostaria de ser chamado?"));
		String newUsuarioNome = newUsuario.get("Por favore, informe o sue nome");
		System.out.println(newUsuarioNome);
		
//		newUsuario.put("Email", "henrique123@gmail.com");
//		String newUsuarioEmail = newUsuario.get("Email");
//		System.out.println(newUsuarioEmail);
	}

}
