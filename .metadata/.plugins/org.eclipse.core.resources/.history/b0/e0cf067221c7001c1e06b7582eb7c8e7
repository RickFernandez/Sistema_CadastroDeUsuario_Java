package br.com.master3.beans;

import java.util.HashMap;

import javax.swing.JOptionPane;

public class MapCadastrarUsuario {

	public static void main(String[] args) {
		HashMap<String, String> newUsuario = new HashMap<String, String>();
		
		newUsuario.put(JOptionPane.showInputDialog("Por favor, informe o seu nome"), JOptionPane.showInputDialog("Por favor, informe seu email"));
		String newUsuarioNome = newUsuario.get("Por favor, informe o seu nome");
		System.out.println(newUsuarioNome);
	}

}
