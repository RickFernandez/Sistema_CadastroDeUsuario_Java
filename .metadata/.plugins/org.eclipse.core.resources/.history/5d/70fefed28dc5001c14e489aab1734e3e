public class TesteConta {

	public static void main(String[] args) {
		Conta cc = new Conta();
		
		if (cc instanceog Conta) {
			System.out.println("cc é do tipo Conta");
		} else {
			System.out.println("cc não é do tipo Conta");
		}

	}

}
